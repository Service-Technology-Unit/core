<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.ucdavis.ucdh.stu.core.beans.LookupTableProperty" table="lookuptableproperty">
        <id name="id" unsaved-value="-1">
            <generator class="native"/>
        </id>
        <property name="tableId"/>
        <property name="sequence"/>
        <property name="name"/>
        <property name="type"/>
        <property name="size"/>
        <property name="label"/>
        <property name="colHeading"/>
        <property name="inputRequired"/>
        <property name="displayOnList"/>
        <property name="source"/>
        <property name="notes"/>
        <property name="creationDate"/>
        <property name="createdBy"/>
        <property name="lastUpdate"/>
        <property name="lastUpdateBy"/>
    </class>

</hibernate-mapping>
